{
  "bar": {
    "position": "top",
    "width": "100%",
    "height": 10,
    "roundness": 0,
    "margins": {
      "top": 0,
      "bottom": 0,
      "left": 0,
      "right": 0
    },
    "backgroundOpacity": 0.8,
    "borderRadius": 0,
    "blur": true,
    "modules": {
      "left": [
        "workspaces",
        "launcher"
      ],
      "center": [
        "window-title"
      ],
      "right": [
        "system-tray",
        "bluetooth",
        "network",
        "audio",
        "battery",
        "clock",
        "power"
      ]
    },
    "modulesSettings": {
      "battery": {
        "criticalThreshold": 15,
        "lowThreshold": 30
      },
      "clock": {
        "format": "%H:%M:%S %d/%m"
      }
    }
  },
  "menus": {
    "launcher": {
      "width": 300,
      "height": 400
    },
    "dashboard": {
      "width": 800,
      "height": 600
    },
    "audio": {
      "width": 300,
      "height": 200
    },
    "bluetooth": {
      "width": 400,
      "height": 300
    },
    "network": {
      "width": 300,
      "height": 200
    },
    "notifications": {
      "width": 400,
      "height": 300
    },
    "power": {
      "width": 200,
      "height": 150
    }
  },
  "settings": {
    "wallpaper": {
      "mode": "fill"
    },
    "keyboard": {
      "layout": "us"
    }
  },
  "notifications": {
    "position": "top-right",
    "margin": 10,
    "width": 350,
    "height": "auto",
    "timeout": 5000
  },
  "bar.customModules.storage.paths": [
    "/"
  ],
  "theme.font.name": "FantasqueSansM Nerd Font Propo",
  "theme.font.style": "normal",
  "theme.font.label": "FantasqueSansM Nerd Font Propo",
  "theme.bar.floating": true,
  "notifications.position": "top right",
  "notifications.timeout": 7000,
  "theme.bar.menus.enableShadow": false,
  "scalingPriority": "hyprland",
  "bar.layouts": {
    "0": {
      "left": [
        "dashboard"
      ],
      "middle": [
        "windowtitle"
      ],
      "right": [
        "network",
        "bluetooth",
        "systray",
        "clock",
        "notifications"
      ]
    },
    "1": {
      "left": [
        "dashboard"
      ],
      "middle": [],
      "right": [
        "clock",
        "notifications"
      ]
    },
    "2": {
      "left": [
        "dashboard",
        "workspaces",
        "windowtitle"
      ],
      "middle": [
        "media"
      ],
      "right": [
        "volume",
        "clock",
        "notifications"
      ]
    }
  },
